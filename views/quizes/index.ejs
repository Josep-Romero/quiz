	<form method="get" action="/quizes">
			<input type="text" name="search" placeholder="Texto a buscar " value="<%= select %>"/>
			<input type="submit" value="Filtrar"/>
			<a href="/quizes/new"><button type="button">Nueva </button></a>
	</form>

	<table>
		<% var i; for (i=0; i < quizes.length; i++) { %>
			<tr>
				<td>
					<% if (quizes[i].tematica==="otro") { %>Otro <% } %>
					<% if (quizes[i].tematica==="humanidades") { %>Humanidades <% } %>
					<% if (quizes[i].tematica==="ocio") { %>Ocio <% } %>
					<% if (quizes[i].tematica==="ciencia") { %>Ciencia <% } %>
					<% if (quizes[i].tematica==="tecnologia") { %>Tecnologia <% } %>
				</td>
				<td><a href="/quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>
				<td><a href="/quizes/<%= quizes[i].id %>/edit"><button>Editar </button></a></td>
				<td>
					<form method="post" action="/quizes/<%= quizes[i].id %>?_method=delete">
						<button type="submit" onClick="return confirm('Â¿Borrar: <%= quizes[i].pregunta %>?');">Borrar </button>
					</form>
				</td>
			</tr>
		<% } %>
	</table><p>
	